<script>
    $(document).on('click.collapse.data-api', '.accordion-toggle', function(event) {
        var $this = $(this),
                parent = $this.data('parent'),
                $parent = parent && $(parent);

        if ($parent) {
            $parent.find('[data-toggle=collapse][data-parent=' + parent + ']').not($this).addClass('collapsed');
        }
    });
</script>

