<div class="row">
    <div class="span3">






        <div class="accordion" id="accordion2">


            {% for i in 0..10 %}
                <div class="accordion-group">
                    <div class="accordion-heading">
                        <a class="accordion-toggle collapsed" data-toggle="collapse" data-parent="#accordion2" href="#collapseid{{ i }}">
                            Категория {{ i }}
                        </a>
                    </div>
                    <div id="collapseid{{ i }}" class="accordion-body collapse">
                        <div class="accordion-inner">
                            <ul class="nav nav-list" style="margin-top: -5px; margin-bottom: -10px; margin-left: -15px; width: 100%;">
                                <li class="active"><a href="#">Активная ссылка</a></li>
                                {% for i in 0..10 %}
                                    <li><a href="#">Ссылка {{ i }}</a></li>
                                {% endfor %}
                            </ul>
                        </div>
                    </div>
                </div>
            {% endfor %}


        </div>


    </div>
    <div class="span7">
        {% for i in 0..10 %}
        <div class="company" style="background: red; width: 700px; height: 130px; margin-bottom: 10px;">
                <img class="thumbnail" data-src="holder.js/260x120" alt="260x120" style="width: 260px; height: 120px; float: left;" src="data:image/png;base64,">
